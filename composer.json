{
	"name" : "hautelook/templated-uri-router",
	"description" : "Symfony2 RFC-6570 compatible router and URL Generator",
	"type" : "symfony-bundle",
	"authors" : [{
			"name" : "Baldur Rensch",
			"email" : "brensch@gmail.com"
		}
	],
	"keywords" : [
		"Symfony2",
		"URI",
		"URL",
		"RFC 6570",
		"JSON",
		"XML",
		"Hateoas",
		"Router"
	],
	"homepage" : "http://www.hautelooktech.com/",
	"license" : [
		"MIT"
	],
	"require" : {
		"symfony/routing" : "~2.1",
		"php" : ">=5.3.0"
	},
	"require-dev" : {
		"symfony/framework-bundle" : "~2.1"
	},
	"autoload" : {
		"psr-0" : {
			"Hautelook\\TemplatedUriRouter" : ""
		}
	},
	"target-dir" : "Hautelook/TemplatedUriRouter",
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1-dev"
		}
